    "# 1 1 2 3 4 9 8 27 16 81 32 243\n",
    "\n",
    "n = int(input())\n",
    "\n",
    "odd = 1 \n",
    "even = 1 \n",
    "\n",
    "for i in range(3,n+1):\n",
    "    if i % 2 == 0:\n",
    "        even = even *3\n",
    "    else:\n",
    "        odd = odd *2\n",
    "\n",
    "if n % 2 == 0:\n",
    "    print(even)\n",
    "else:\n",
    "    print(odd)"
