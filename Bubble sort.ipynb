    "arr = [4,45,32,123,432,2,55,34,232,231]\n",
    "\n",
    "for num in range(len(arr)-1,0,-1):\n",
    "    for i in range(num):\n",
    "        if arr[i] > arr[i+1]:\n",
    "            temp = arr[i]\n",
    "            arr[i] = arr[i+1]\n",
    "            arr[i+1] = temp\n",
    "            \n",
    "            \n",
    "print(arr)"
